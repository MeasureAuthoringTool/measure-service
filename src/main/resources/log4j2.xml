<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
  <Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">

      <PatternLayout pattern=" %d{yyyy-MM-dd HH:mm:ss.SSS} [%level] [thread-id %T] %c - %msg%n" />/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
</Configuration>